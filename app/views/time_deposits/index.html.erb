<div class="row">
<div class="col-md-12">
<div class="box box-solid">
<div class="box-header with-border">
  <h4 class="text-muted"> Time Deposits</h4>
</div>
<div class="box-body">
  <div class="table table-responsive">
    <table class="table table-striped">
      <thead>
        <tr>
          <th> Member </th>
          <th> Account # </th>
          <th> Type </th>
          <th><span class="pull-right"> Balance </span> </th>
        </tr>
      </thead>
      <tbody>
        <% @time_deposits.each do |time_deposit| %>
        <tr>
            <td><%=link_to image_tag(time_deposit.member.avatar.url(:small), class: "img-circle"), time_deposit.member%> &nbsp; &nbsp; <%=link_to time_deposit.member.try(:first_and_last_name), time_deposit.member %></td>
            <td><%= time_deposit.account_number.upcase %></td>
            <td> <%= time_deposit.time_deposit_product_name %></td>
            <td><span class="pull-right"><%=number_to_currency time_deposit.balance %></span></td>
              </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
</div>
