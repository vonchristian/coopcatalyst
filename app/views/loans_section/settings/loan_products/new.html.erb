<div class="container">
<div class="box box-solid">
<div class="box-header with-border">
<h4>Loan Product Details</h4>
      </div>
      <div class="box-body">
       <%= simple_form_for @loan_product, url: :loans_section_loan_products  do |f| %>
      <%=f.error_notification %>
<div class="col-md-6">
</div>
<div class="col-md-5">
      <%= f.input :name %>
      <%= f.input :interest_rate %>
      <%= f.input :term %>



      <div class="modal-footer">
          <%=f.submit 'Save', class: 'btn btn-primary', data: { disable_with: "Saving..."} %>
        </div>
      <% end %>
    </div>
  </div>
</div>
</div>
