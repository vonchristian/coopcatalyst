<div class="container">
<div class="box box-solid">
<div class="box-header with-border">
<h4>Savings Product Details</h4>
      </div>
      <div class="box-body">
       <%= simple_form_for @savings_product, url: :coop_services_savings_products  do |f| %>
      <%=f.error_notification %>
<div class="col-md-6">
</div>
<div class="col-md-5">
      <%= f.input :name %>
      <%= f.input :interest_rate %>


      <div class="modal-footer">
          <%=f.submit 'Save', class: 'btn btn-primary', data: { disable_with: "Saving..."} %>
        </div>
      <% end %>
    </div>
  </div>
</div>
</div>
