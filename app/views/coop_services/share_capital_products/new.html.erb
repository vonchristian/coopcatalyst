<div class="container">
<div class="box box-solid">
<div class="box-header with-border">
<h4>Share Capital Product Details</h4>
      </div>
      <div class="box-body">
       <%= simple_form_for @share_capital_product, url: :coop_services_share_capital_products  do |f| %>
      <%=f.error_notification %>
<div class="col-md-6">
</div>
<div class="col-md-5">
      <%= f.input :minimum_paid_up_share_capital %>

      <div class="modal-footer">
          <%=f.submit 'Save', class: 'btn btn-primary', data: { disable_with: "Saving..."} %>
        </div>
      <% end %>
    </div>
  </div>
</div>
</div>
